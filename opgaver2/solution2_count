void setup(){
 println(countChar("abcdahahaah",'a')); //udskriver 5
 println(countChar("abcdahahaah",'h')); //udskriver 3
 println(countChar("abcdahahaah",'d')); //udskriver 1
}

int countChar(String w, char c){
  int count = 0;
  for(int i=0; i<w.length() ; i++){
    if(w.charAt(i)==c){ 
      count++; 
    }
 }
  
  return count;
}